{
  "name": "function-ui",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://github.com/vprjapati-Lab49/funtion",
  "scripts": {
    "dev": "webpack-dev-server --open --config ./config/dev.js",
    "dist": "npm run copy && webpack --config ./config/dist.js --progress",
    "infra": "NODE_ENV=infra webpack-dev-server --port 9091 --open --config ./config/dist.js",
    "prod": "NODE_ENV=production webpack-dev-server --port 9091 --open --config ./config/dist.js",
    "lint": "tslint -c 'tslint.json' './src/**/*.ts'",
    "test:watch": "node ./node_modules/.bin/jest --watch",
    "test:dist": "node ./node_modules/.bin/jest --ci --json --outputFile=test-results.json",
    "copy": "npm run clean && copyfiles -f ./docker/* ./dist && copyfiles -u 1 \"./src/assets/**/*\" \"./dist/build\"",
    "clean": "rimraf dist/*",
    "build": "npm run dist",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "@material-ui/core": "^1.4.0",
    "@types/lodash": "^4.14.136",
    "@types/node": "^12.6.2",
    "bowser": "^1.7.1",
    "classnames": "^2.2.6",
    "file-saver": "^1.3.3",
    "gh-pages": "^2.0.1",
    "highcharts": "^6.1.2",
    "history": "^4.7.2",
    "isemail": "^3.1.1",
    "jss": "^9.8.7",
    "jss-preset-default": "^4.5.0",
    "lodash": "^4.17.13",
    "moment": "^2.20.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-dom-factories": "^1.0.2",
    "react-inlinesvg": "^0.8.1",
    "react-jss": "^8.6.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-text-mask": "^5.4.3",
    "redux": "^3.7.2",
    "redux-history-transitions": "^2.2.0",
    "redux-logger": "^3.0.6",
    "redux-observable": "^0.17.0",
    "reselect": "^3.0.1",
    "rxjs": "^5.5.6",
    "typesafe-actions": "^1.1.2"
  },
  "devDependencies": {
    "@kwonoj/rxjs-testscheduler-compat": "^1.0.2",
    "@types/enzyme": "^3.1.6",
    "@types/file-saver": "^1.3.0",
    "@types/jest": "^24.0.15",
    "@types/jss": "^9.5.4",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/react-hot-loader": "^3.0.5",
    "@types/react-redux": "^5.0.14",
    "@types/react-router-dom": "^4.2.3",
    "@types/react-router-redux": "^5.0.11",
    "@types/react-test-renderer": "^16.0.0",
    "@types/redux-logger": "^3.0.5",
    "autoprefixer": "^7.2.4",
    "copyfiles": "^1.2.0",
    "css-loader": "^0.28.8",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "file-loader": "^1.1.6",
    "fork-ts-checker-webpack-plugin": "^0.4.2",
    "fs": "0.0.1-security",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^23.0.0",
    "minimist": "^1.2.0",
    "open": "^0.0.5",
    "path": "^0.12.7",
    "postcss-loader": "^2.0.10",
    "react-hot-loader": "^3.1.3",
    "react-switch": "^2.3.2",
    "rimraf": "^2.6.2",
    "style-loader": "^0.19.1",
    "ts-jest": "^22.0.1",
    "ts-loader": "^3.5.0",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.4.2",
    "tslint-loader": "^3.5.3",
    "tslint-react": "^3.3.3",
    "typescript": "^3.5.3",
    "uglify-es": "3.3.9",
    "uglifyjs-webpack-plugin": "^1.2.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.11.0",
    "webpack-combine-loaders": "^2.0.3",
    "webpack-dev-server": "^2.11.2"
  },
  "jest": {
    "mapCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.ts",
    "testMatch": [
      "<rootDir>/src/**/?(*.)(spec|test).ts?(x)"
    ],
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$"
    ],
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json"
      }
    }
  }
}
